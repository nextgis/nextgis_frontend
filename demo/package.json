{
  "name": "@nextgis/demo-app",
  "version": "3.0.0",
  "description": "Demo site for NextGIS Frontend libraries",
  "productName": "demo-app",
  "author": "NextGIS <info@nextgis.com>",
  "type": "module",
  "private": true,
  "_priority": 1,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "gen:examples": "node ./scripts/updateDemoExamples.js",
    "start": "yarn gen:examples && quasar dev",
    "build": "yarn gen:examples && quasar build",
    "serve": "quasar serve --history",
    "docker": "yarn gen:examples && docmypkg -r harbor.nextgis.net -n frontend/code -l -d ./docker"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.12",
    "highlight.js": "11.8.0",
    "marked": "^12.0.1",
    "marked-highlight": "2.0.6",
    "quasar": "^2.16.5",
    "simple-code-editor": "^2.0.9",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "overrides": {
    "simple-code-editor": {
      "highlight.js": "11.8.0"
    }
  },
  "devDependencies": {
    "@quasar/app-vite": "^2.0.0-beta.14",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.2",
    "chalk": "^5.3.0",
    "docmypkg": "^0.0.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.3.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
