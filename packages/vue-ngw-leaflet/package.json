{
  "name": "@nextgis/vue-ngw-leaflet",
  "version": "1.0.0-alpha.2",
  "description": "Vue ngw map component for leaflet",
  "main": "lib/index.js",
  "module": "lib/vue-ngw-leaflet.esm-bundler.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@nextgis/leaflet-map-adapter": "^1.0.0-alpha.2",
    "@nextgis/vue-ngw-map": "^1.0.0-alpha.2"
  },
  "devDependencies": {
    "@nextgis/build-tools": "file:../build-tools"
  },
  "buildOptions": {
    "name": "VueNgwLeaflet",
    "injectCss": true,
    "formats": [
      "esm-bundler"
    ]
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "dev": "node ../build-tools/lib/build.js",
    "prod": "npm run dev -- --release",
    "lint": "eslint ./src/**/*.ts --fix --c ../../.eslintrc",
    "watch": "npm run dev -- --watch"
  },
  "keywords": [
    "NextGIS",
    "MAP"
  ],
  "author": "NextGIS",
  "files": [
    "index.js",
    "lib"
  ],
  "license": "LGPL-3.0",
  "homepage": "https://github.com/nextgis/nextgis_frontend/tree/master/packages/vue-ngw-leaflet#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextgis/nextgis_frontend.git"
  },
  "bugs": {
    "url": "https://github.com/nextgis/nextgis_frontend/issues"
  }
}
