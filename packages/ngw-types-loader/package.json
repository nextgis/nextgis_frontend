{
  "name": "@nextgis/ngw-types-loader",
  "version": "3.0.0-alpha.12",
  "description": "NGW TypeScript Declaration Loader",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    }
  },
  "bin": "./lib/index.cjs",
  "scripts": {
    "clean": "rimraf lib",
    "build:cjs": "tsc --outDir lib --module commonjs && npm run rename:cjs",
    "build:esm": "tsc --outDir lib --module esnext",
    "rename:cjs": "node -e \"require('fs').renameSync('./lib/index.js', './lib/index.cjs')\"",
    "prod": "npm run clean && npm run build:cjs && npm run build:esm",
    "test": "echo \"No tests yet\""
  },
  "devDependencies": {
    "rimraf": "^6.0.0",
    "typescript": "^5.5.0"
  },
  "license": "MIT"
}
