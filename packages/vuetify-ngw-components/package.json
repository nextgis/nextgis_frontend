{
  "name": "@nextgis/vuetify-ngw-components",
  "version": "1.0.0-alpha.0",
  "description": "Vuetify components for NGW Frontend",
  "main": "index.js",
  "module": "lib/vuetify-ngw-components.esm-bundler.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@nextgis/leaflet-map-adapter": "^1.0.0-alpha.0",
    "@nextgis/ngw-map": "^1.0.0-alpha.0",
    "@nextgis/utils": "^1.0.0-alpha.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^9.0.0",
    "vuetify": "^2.3.6"
  },
  "devDependencies": {
    "@nextgis/build-tools": "file:../build-tools"
  },
  "buildOptions": {
    "name": "VuetifyNgwComponents",
    "formats": [
      "esm-bundler"
    ]
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "dev": "node ../build-tools/lib/build.js",
    "prod": "npm run dev -- --release",
    "lint": "eslint ./src/**/*.ts --fix --c ../../.eslintrc",
    "watch": "npm run dev -- --watch"
  },
  "keywords": [
    "NextGIS",
    "MAP"
  ],
  "author": "NextGIS",
  "files": [
    "index.js",
    "lib"
  ],
  "license": "LGPL-3.0",
  "homepage": "https://github.com/nextgis/nextgis_frontend/tree/master/packages/vuetify-ngw-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextgis/nextgis_frontend.git"
  },
  "bugs": {
    "url": "https://github.com/nextgis/nextgis_frontend/issues"
  }
