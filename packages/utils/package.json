{
  "name": "@nextgis/utils",
  "version": "0.32.0",
  "description": "Development tools",
  "main": "index.js",
  "module": "lib/utils.esm-bundler.js",
  "unpkg": "lib/utils.global.prod.js",
  "jsdelivr": "lib/utils.global.prod.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@nextgis/build-tools": "file:../build-tools"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "dev": "npm run clean && webpack --config ./webpack.config.js --progress",
    "build": "npm run dev -- --mode=production",
    "prod": "node ../build-tools/lib/rollup.build.js",
    "lint": "eslint ./src/**/*.ts --fix --c ../../.eslintrc",
    "watch": "npm run dev -- --watch"
  },
  "keywords": [
    "NextGIS",
    "MAP"
  ],
  "author": "NextGIS",
  "files": [
    "index.js",
    "lib"
  ],
  "buildOptions": {
    "name": "Utils",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "license": "LGPL-3.0",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextgis/nextgis_frontend.git"
  },
  "bugs": {
    "url": "https://github.com/nextgis/nextgis_frontend/issues"
  }
}
