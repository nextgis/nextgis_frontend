  "main": "lib/dialog.global.prod.js",
  "module": "lib/dialog.esm-bundler.js",
  "unpkg": "lib/dialog.global.prod.js",
  "jsdelivr": "lib/dialog.global.prod.js",
  "types": "lib/index.d.ts",
